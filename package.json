{
  "dependencies": {
    "@toolbuilder/make-factory": "^0.1.0",
    "esm": "^3.2.25"
  },
  "description": "Small (1.2kb gz), set of functions like range, map, reduce, filter, zip, for iterable objects.",
  "devDependencies": {
    "@toolbuilder/eslint-config": "^0.1.0",
    "documentation": "^12.0.3",
    "eslint": "^6.3.0",
    "markedpp": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "rollup": "^1.17.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "tap-nirvana": "^1.1.0",
    "tape": "^4.11.0",
    "tape-run": "^6.0.1",
    "zora": "^3.0.3"
  },
  "eslintConfig": {
    "extends": "@toolbuilder/eslint-config"
  },
  "files": [
    "src",
    "index.js",
    "umd"
  ],
  "license": "MIT",
  "keywords": [
    "iterable",
    "iterator",
    "generator",
    "chainable",
    "esm",
    "functional",
    "transducers"
  ],
  "main": "index.js",
  "module": "src/chainable.js",
  "name": "iterablefu",
  "unpkg": "src/chainable.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/toolbuilder/iterablefu"
  },
  "scripts": {
    "build": "npm-run-all --serial test lint bundle test:browser docs",
    "bundle": "npm-run-all --serial bundle:clean bundle:umd bundle:test",
    "bundle:clean": "rimraf umd temp",
    "bundle:umd": "rollup -c rollup.config.js",
    "bundle:test": "node -r esm scripts/forbrowser.js",
    "docs": "npm-run-all --serial docs:clean docs:generate",
    "docs:clean": "rimraf docs",
    "docs:generate": "node -r esm scripts/docgen.js",
    "lint": "eslint src scripts test",
    "prepublishOnly": "npm run build",
    "test": "tape -r esm test/*_test.js | tap-nirvana",
    "test:browser": "cat temp/bundle.js | tape-run | tap-nirvana"
  },
  "version": "0.4.0-rc0"
}
